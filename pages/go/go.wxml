<!--go.wxml-->
<view class="flex-style">
  <text class="fcolor-red">{{textData}}</text>
  <text style="display:{{style_show}};">{{sName}}  -  {{eName}}</text>
</view>
<view class="map_container" style="top:{{style_top}};" bindtouchend='startItv' bindtouchstart='stopItv'>
  <map class="map" id="map" longitude="{{driv_longitude}}" latitude="{{driv_latitude}}" scale="12" show-location="true" markers="{{markers}}" polyline='{{polyline}}'></map>
</view>
<view class="btns">
  <view id="receive-now" class="receive-now" catchtap="received">确认已接到乘客</view>
  <view class="cancel-btn" catchtap="cancelOrder">取消订单</view>
</view>